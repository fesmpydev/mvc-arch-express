<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Users</title>
  </head>
  <body>
    <header>
      <h1>List of users</h1>
    </header>

    <main>
      <section>
        <ul>
          <% users.results.forEach(user => { %>
          <li>
            <div>
              <h2><%= user.name.first %> <%= user.name.last %></h2>
              <img src="<%= user.picture.large %>" alt="User image" />
              <p><%= user.email %></p>
            </div>
          </li>
          <% }) %>
        </ul>
      </section>
    </main>
  </body>
</html>
